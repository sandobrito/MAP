package padraoAdapter;

public class ImagemAdapter extends ImagemBMP implements ImagemTarget{

	@Override
    public void carregarImagem(String nomeDoArquivo) {
        carregarImagem(nomeDoArquivo);
    }
 
    @Override
    public void desenharImagem(int posX, int posY, int largura, int altura) {
    	desenharImagem(posX, posY, altura, altura);
    }

	
}
