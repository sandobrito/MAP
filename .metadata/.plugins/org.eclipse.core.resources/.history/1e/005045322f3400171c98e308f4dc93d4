package strategy;

public enum TipoGratificacao {

	GERENTE{
		public Gratificacao obterGratificacao(){

			return new GratificacaoGerente();
		}
	},
	DESENVOLVEDOR{
		public Gratificacao obterGratificacao(){

			return new GratificacaoDesenvolvedor();
		}
	},
	LIDER{
		public Gratificacao obterGratificacao(){

			return new GratificacaoLider();
		}
	},
	DBA{
		public Gratificacao obterGratificacao(){

			return new GratificacaoDBA();
		}
	},;

	
	
	
}
